{
  "name": "spotenu-backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev:server": "ts-node-dev -r tsconfig-paths/register --transpile-only --no-notify --ignore-watch node_modules  src/shared/infra/http/index.ts",
    "migrate:latest": "knex migrate:latest --knexfile src/shared/infra/knex/knexfile.ts",
    "migrate:rollback": "knex migrate:rollback --knexfile src/shared/infra/knex/knexfile.ts",
    "migrate:make": "knex migrate:make -x ts --knexfile src/shared/infra/knex/knexfile.ts"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "knex": "^0.21.2"
  },
  
  "devDependencies": {
    "@types/knex": "^0.16.1",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "ts-node-dev": "^1.0.0-pre.56",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.7"
  }
}
